<h1>ADD TESTIMONIAL</h1>

<form id="testimonialForm" enctype="multipart/form-data">
    <!-- Input for Name -->
    <label for="testimonialName">Name:</label>
    <input type="text" id="testimonialName" name="Testimonial Name" required>
    <br>

    <!-- Input for Description -->
    <label for="testimonialDescription">Description:</label>
    <textarea id="testimonialDescription" name="Testimonial Description" rows="4" required></textarea>
    <br>

    <!-- Input for Photo -->
    <label for="testimonialPhoto">Photo:</label>
    <input type="file" id="testimonialPhoto" name="TestimonialPhoto" accept="image/*" required>
    <br>

    <!-- Submit Button -->
    <button type="button" id="addTestimonialBtn" onclick="handleTestimonial()">Submit</button>
</form>